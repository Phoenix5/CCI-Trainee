@using Employee_Summary.Models
@model Employee_Summary.Models.Summary

@{
    ViewBag.Title = "TechSkills";
    Layout = "~/Views/EmployeeReview/layout/_Layout.cshtml";
}

<h2>TechSkills</h2>

@Model.Id
@Model.Name
@Model.Designation

@Html.DisplayFor(model => model.Id)
@Html.DisplayFor(model => model.Name)
@Html.DisplayFor(model => model.Designation)
@Html.DisplayFor(model => model.set)
@Html.DisplayFor(model => model.Dskill.CodingSkills)
@Html.DisplayFor(model => model.Dskill.QualityAssurance)
@Html.DisplayFor(model => model.Dskill.TimeLogging)
@Html.DisplayFor(model => model.Dskill.TroubleShootingSkills)
@Html.DisplayFor(model => model.Tskill.AngularJS)
@Html.DisplayFor(model => model.Tskill.ASPDotNETCORE)
@Html.DisplayFor(model => model.Tskill.CSharp)
@Html.DisplayFor(model => model.Tskill.GIT)
@Html.DisplayFor(model => model.Tskill.Ruby)
@Html.DisplayFor(model => model.Tskill.TFS)




@using (Html.BeginForm("Category", "EmployeeReview", FormMethod.Post, new { @class = "form-horizontal", role = "form" }))
{
    @Html.AntiForgeryToken()


    <div class="form-group">
        @Html.HiddenFor(model => model.Id)
        @Html.HiddenFor(model => model.Name)
        @Html.HiddenFor(model => model.Designation)
        @Html.HiddenFor(model => model.set)
        <div class="col-md-10">
            @Html.LabelFor(model => model.Tskill.AngularJS, htmlAttributes: new { @class = "control-label col-md-2" })

            @Html.DropDownListFor(model => model.Tskill.AngularJS, new SelectList(Enum.GetValues(typeof(Tech_Rating))), "Select Designation")
        </div>
        <br />
        <div class="col-md-10">
            @Html.LabelFor(model => model.Tskill.ASPDotNETCORE, htmlAttributes: new { @class = "control-label col-md-2" })

            @Html.DropDownListFor(model => model.Tskill.ASPDotNETCORE, new SelectList(Enum.GetValues(typeof(Tech_Rating))), "Select Designation")
        </div>
        <br />

        <div class="col-md-10">
            @Html.LabelFor(model => model.Tskill.CSharp, htmlAttributes: new { @class = "control-label col-md-2" })

            @Html.DropDownListFor(model => model.Tskill.CSharp, new SelectList(Enum.GetValues(typeof(Tech_Rating))), "Select Designation")
        </div>
        <br />

        <div class="col-md-10">
            @Html.LabelFor(model => model.Tskill.GIT, htmlAttributes: new { @class = "control-label col-md-2" })

            @Html.DropDownListFor(model => model.Tskill.GIT, new SelectList(Enum.GetValues(typeof(Tech_Rating))), "Select Designation")
        </div>
        <br />

        <div class="col-md-10">
            @Html.LabelFor(model => model.Tskill.Ruby, htmlAttributes: new { @class = "control-label col-md-2" })

            @Html.DropDownListFor(model => model.Tskill.Ruby, new SelectList(Enum.GetValues(typeof(Tech_Rating))), "Select Designation")
        </div>
        <br />

        <div class="col-md-10">
            @Html.LabelFor(model => model.Tskill.TFS, htmlAttributes: new { @class = "control-label col-md-2" })

            @Html.DropDownListFor(model => model.Tskill.TFS, new SelectList(Enum.GetValues(typeof(Tech_Rating))), "Select Designation")
        </div>
        <br />
        <div class="col-md-offset-2 col-md-10">
            <input type="submit" value="Done" class="btn btn-default" />
        </div>
    </div>
}